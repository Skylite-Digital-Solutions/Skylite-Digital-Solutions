{
  "hosting": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
            {
              "origin": ["http://localhost:5173"],
              "method": ["GET", "POST", "PUT", "DELETE"],
              "maxAgeSeconds": 3600
            }
        ]
      }
    ]
  }
}
